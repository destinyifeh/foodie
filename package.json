{
  "name": "original",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "prebuildRelease": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "prebuildDebug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "assembleDebug": "cd android && ./gradlew assembleDevDebug",
    "assembleQaRelease": "./gradlew assembleQaRelease",
    "assembleProdRelease": "cd android && ./gradlew assembleProdRelease",
    "bundleRelease": "cd android && ./gradlew bundleProdRelease",
    "deployStaging": "appcenter codepush release-react -a destechofficial-gmail.com/original -d Staging",
    "deployProd": "appcenter codepush release-react -a destechofficial-gmail.com/original -d Production",
    "promote": "appcenter codepush promote -s Staging -d Production -a destechofficial-gmail.com/original"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-animatable": "^1.3.3",
    "react-native-bootsplash": "^4.6.0",
    "react-native-code-push": "^7.1.0",
    "react-native-config": "^1.5.0",
    "react-native-device-info": "^10.6.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-maps": "^1.4.0",
    "react-native-paper": "^5.5.2",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.8",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
